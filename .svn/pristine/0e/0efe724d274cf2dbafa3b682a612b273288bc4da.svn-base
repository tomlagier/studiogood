/*Social Tabs*/

#social-nav.open .sidebar-tabs
{
	left: 300px;
}

#instagram-tab
{
	background: rgba(0,0,0,.8) url('/img/instagram-48.png') no-repeat right;
	background-size: 40px;
}

#instagram-tab.active, #instagram-tab:hover
{
	background: rgba(0,0,0,.8) url('/img/instagram-48-active.png') no-repeat right;
	background-size: 40px;
}

.sidebar-tab:hover
{
	width: 48px;
}

.sidebar-slide
{
	width: 0;
	background: rgba(0,0,0,.72);
	height: 100%;
	position: fixed;
	top: 0;
	left: -300px;
	z-index: 98;
	transition: all .3s;
	-webkit-transition: all .3s;
	-moz-transition: all .3s;
	width: 300px;
	overflow: hidden;
}

.sidebar-slide .inner
{
	width: 319px;
	padding: 0 25px 10px 10px;
	margin-top: 10px;
	overflow: scroll;
	transition: all .3s;
	-webkit-transition: all .3s;
	-moz-transition: all .3s;
	opacity: 1;
	overflow-x: hidden;
}

.sidebar-slide .inner.fade
{
	opacity: 0;
}

#social-nav.open .sidebar-slide
{
	left: 0;
}

