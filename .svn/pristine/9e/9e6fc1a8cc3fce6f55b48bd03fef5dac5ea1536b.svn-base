/*Social Tabs*/
.sidebar-tabs
{
	position: fixed;
	left: 0;
	top: 40%;
	z-index: 9999;
	opacity: .9;
	transition: all .3s;
	-webkit-transition: all .3s;
	-moz-transition: all .3s;
}

#social-nav.open .sidebar-tabs
{
	left: 300px;
}

.sidebar-tab
{
	width: 40px;
	height: 40px;
	margin-top: 5px;
	border-radius: 0 5px 5px 0;
	transition: all .3s;
	-webkit-transition: all .3s;
	-moz-transition: all .3s;
	cursor: pointer;
}

#twitter-tab
{
	background: rgba(0,0,0,.8) url('/img/twitter-48.png') no-repeat right;
	background-size: 40px;
}

#twitter-tab.active, #twitter-tab:hover
{
	background: rgba(0,0,0,.8) url('/img/twitter-48-active.png') no-repeat right;
	background-size: 40px;
}

#facebook-tab
{
	background: rgba(0,0,0,.8) url('/img/facebook-48.png') no-repeat right;
	background-size: 40px;
}

#facebook-tab.active, #facebook-tab:hover
{
	background: rgba(0,0,0,.8) url('/img/facebook-48-active.png') no-repeat right;
	background-size: 40px;
}

#instagram-tab
{
	background: rgba(0,0,0,.8) url('/img/instagram-48.png') no-repeat right;
	background-size: 40px;
}

#instagram-tab.active, #instagram-tab:hover
{
	background: rgba(0,0,0,.8) url('/img/instagram-48-active.png') no-repeat right;
	background-size: 40px;
}

.sidebar-tab:hover
{
	width: 48px;
}

.sidebar-slide
{
	width: 0;
	background: rgba(0,0,0,.72);
	height: 100%;
	position: fixed;
	top: 0;
	left: -300px;
	z-index: 98;
	transition: all .3s;
	-webkit-transition: all .3s;
	-moz-transition: all .3s;
	width: 300px;
	overflow: hidden;
}

.sidebar-slide .inner
{
	width: 319px;
	padding: 0 25px 10px 10px;
	margin-top: 10px;
	overflow: scroll;
	transition: all .3s;
	-webkit-transition: all .3s;
	-moz-transition: all .3s;
	opacity: 1;
	overflow-x: hidden;
}

.sidebar-slide .inner.fade
{
	opacity: 0;
}

#social-nav.open .sidebar-slide
{
	left: 0;
}

#hidden-slides
{
	display: none;
