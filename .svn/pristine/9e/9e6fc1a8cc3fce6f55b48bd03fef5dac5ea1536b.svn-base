.social-entry .social-message
{
	font-size: 1.05em;
	color: white;
	font-family: 'Pathway Gothic One', 'Helvetica Neue', Helvetica, Helvetica, Arial, sans-serif;
}

.social-entry .social-image
{
	width: 80px;
	height: 80px;
	border-radius: 50%;
}

.social-wrapper:nth-child(2n+1) .social-image
{
	float: right;
	margin: 0 0 8px 8px;
}

.social-wrapper:nth-child(2n+1):after
{
	content: "";
	position: absolute;
	bottom: 2px;
	right: 2px;
	border-left: 5px solid transparent;
	border-right: 5px solid transparent;
	border-bottom: 15px solid #0d5cbf;
	-webkit-transform: rotate(132deg);
}

.social-wrapper:nth-child(2n) .social-image
{
	float: left;
	margin: 0 8px 8px 0;
}

.social-wrapper:nth-child(2n):after
{
	content: "";
	position: absolute;
	bottom: 2px;
	left: 2px;
	border-left: 5px solid transparent;
	border-right: 5px solid transparent;
	border-bottom: 15px solid #0d5cbf;
	-webkit-transform: rotate(222deg);
}

